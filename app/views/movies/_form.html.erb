<%= form_with model: movie, local:true do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_area :title, class: "form-control", rows: "7" %>
  </div>
  <p>
    <%= f.submit class: "btn btn-block btn-primary" %>
  </p>
  <p>
    <%= link_to '戻る', :back, class: "btn btn-block btn-secondary" %>
  </p>
<% end %>
